<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="br.com.bitwiselab.jano.model">
	<class name="Contato" table="CONTATO">
		<id name="id" column="IDCONTATO" access="field" unsaved-value="null" type="long">
			<generator class="sequence">
				<param name="sequence">s_contato</param>
			</generator>
		</id>
		<property name="telefone" column="TELEFONE" />
		<property name="celular" column="CELULAR" />
		<property name="email" column="EMAIL" />
	</class>
</hibernate-mapping>