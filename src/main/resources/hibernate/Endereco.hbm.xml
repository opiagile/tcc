<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="br.com.bitwiselab.jano.model">
	<class name="Endereco" table="ENDERECO">
		<id name="id" column="IDENDERECO" access="field" unsaved-value="null" type="long">
			<generator class="sequence">
				<param name="sequence">s_endereco</param>
			</generator>
		</id>
		<property name="numero" column="NUMERO" />
		<property name="complemento" column="COMPLEMENTO" />
		<many-to-one name="logradouro" column="IDLOGRADOURO"
			class="br.com.bitwiselab.jano.model.Logradouro" not-null="true" lazy="false" />
	</class>
</hibernate-mapping>