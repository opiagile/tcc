<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="br.com.bitwiselab.jano.model">
	<class name="TipoEquipamento" table="TIPOEQUIPAMENTO">
		<id name="id" column="IDTIPOEQUIPAMENTO" access="field" unsaved-value="null" type="long">
			<generator class="native" />
		</id>
		<property name="descricao" column="DESCRICAO" not-null="true" />
	</class>
</hibernate-mapping>